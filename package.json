{
  "name": "cheat-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "repository": "https://github.com/weronikaolejniczak/chEAT-frontend.git",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
